package net.satcy.util{	import net.satcy.util.MouseEventUtil;	/*		@author hori@satcy.net, satoshi@rhizomatiks.com				MouseEventUtilの拡張、管理するクラス		一気に、リスナーをremoveしたり、指定してresumeしたり、出来るようにする				@Function resume(Array = null):void		@Function pause(Array = null):void		@discription Arrayで指定したもの以外に適用する				@Function numDict():uint				@see MouseEventUtil	*/	public class MouseEventMg extends MouseEventUtil{				public static function resume(__cancel_arr:Array = null):void{			for(var i in dict){				var w_num:int = 0;				if(__cancel_arr){					for(var j in __cancel_arr){						if(dict[i] == __cancel_arr[j]){							w_num++;						}					}				}				if(w_num == 0){					addOn(i, true, dict[i]);				}			}		}				public static function pause(__cancel_arr:Array = null):void{			for(var i in dict){				var w_num:int = 0;				if(__cancel_arr){					for(var j in __cancel_arr){						if(dict[i] == __cancel_arr[j]){							w_num++;						}					}				}				if(w_num == 0){					removeOn(i);				}			}		}				public static function numDict():uint{			var num:uint = 0;			for(var i in dict){				num++;			}			return num;		}				public static function targetArray():Array{			var arr:Array = new Array();			for(var i in dict){				arr.push(i);			}			return arr;		}				public static function targetArrayByName():Array{			var arr:Array = new Array();			for(var i in dict){				arr.push(i.name);			}			return arr;		}	}	}