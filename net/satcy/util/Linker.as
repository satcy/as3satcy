package net.satcy.util{		////static method		/*ex.	import flash.external.ExternalInterface;	import net.satcy.util.Linker;	Linker.getURL("http://satcy.net/", "_blank");	*/		import flash.net.navigateToURL;    import flash.net.URLRequest;    import flash.net.URLVariables;	import flash.external.ExternalInterface;		public class Linker{				public static function getURL(__url:String, target:String = "_self") {			if (__url.match(/mailto:/i)) {				if (!ExternalInterface.available) return;				ExternalInterface.call(					'function() {' +						'location.href="' + __url + '"' + 					'}'				);			} else {				var request:URLRequest = new URLRequest(__url);				try {            					navigateToURL(request, target);				}				catch (e:Error) {					// handle error here					trace("DISABLE getURL");				}			}		}					}		}