package net.satcy.util{	import flash.utils.getDefinitionByName;	public function getObjectFromClassName(_class:String, ...p):Object	{			var obj:Object;		switch(p.length){			case 0:				obj = new (getDefinitionByName(_class) as Class)();				break;			case 1:				obj = new (getDefinitionByName(_class) as Class)(p[0]);				break;			case 2:				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1]);				break;			case 3: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2]);				break;			case 4: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3]);				break;			case 5: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4]);				break;			case 6: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5]);				break;			case 7: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6]);				break;			case 8: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7]);				break;			case 9: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7], p[8]);				break;			case 10: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7], p[8], p[9]);				break;			case 11: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7], p[8], p[9], p[10]);				break;			case 12: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7], p[8], p[9], p[10], p[11]);				break;			case 13: 				obj = new (getDefinitionByName(_class) as Class)(p[0], p[1], p[2], p[3], p[4], p[5], p[6], p[7], p[8], p[9], p[10], p[11], p[12]);				break;		}		return obj;	}}