	import flash.display.BitmapData;
	import flash.geom.Point;
		
	public function drawBitmapString(str:String, x:Number, y:Number):BitmapData
	{
		var cw:int = 8;
		var ch:int = 16;
		var target:BitmapData = new BitmapData(str.length*cw, ch, true, 0);
		var bf:BitmapFont = BitmapFont.getInstance();
		var tx:Number = 0;
		for ( var i:int = 0; i<str.length; i++ ) {
			var bmp:BitmapData = bf.getBitmapData(str.charAt(i));
			
			if ( bmp ) {
				target.copyPixels(bmp, bmp.rect, new Point(tx + x, y));
			} 
			tx += cw;
		}
		return target;
	}

}
