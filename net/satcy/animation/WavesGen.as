package net.satcy.animation{	public class WavesGen{		private var w_arr:Array;		private var arr_len:int;		public function WavesGen(...objs){			w_arr = new Array();			for(var i in objs){				var wp:WaveParticle = new WaveParticle(objs[i]["freq"], objs[i]["phase"]);				w_arr.push(wp);			}			arr_len = w_arr.length;		}		public function update():Number{			var gen:Number = 0;			for(var i:int = 0; i<arr_len;i++){				if(i==0) {					gen = w_arr[i].update();				}else{					gen *= w_arr[i].update();				}			}			return gen;		}				public function destroy():void{			w_arr = null;		}	}	}