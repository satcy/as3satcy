package net.satcy.data{	import flash.events.Event;	/*		@author satoshi@rhizomatiks.com		@discription 		@see 	*/	public class PreLoadAssetEvent extends Event{				public static const ON_PROGRESS : String = "onProgress";		public static const ON_COMPLETE : String = "onComplete";		private var _maxCount : int;		private var _count : int;		private var _percent : uint;		public function PreLoadAssetEvent (type : String, bubbles : Boolean = false, cancelable : Boolean = false){			super (type, bubbles, cancelable);		}				public function get maxCount():int{			return _maxCount;		}		public function set maxCount(d:int):void{			_maxCount = d;		}				public function get count():int{			return _count;		}		public function set count(d:int):void{			_count = d;		}				public function get percent():uint{			return _percent;		}		public function set percent(d:uint):void{			_percent = d;		}	}	}